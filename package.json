{
	"name": "pro-mern-stack-ui",
	"description": "Pro MERN Stack UI",
	"main": "index.js",
	"engines": {
		"node": "15.x",
		"npm": "7.x"
	},
	"scripts": {
		"#start": "UI server. HMR is enabled in dev mode.",
		"start": "node dist/server.js",
		"#lint": "Runs ESLint on all relevant files",
		"lint": "eslint . --ext js,jsx --ignore-pattern public",
		"#watch-server-hmr": "Recompile server HMR bundle on changes.",
		"watch-server-hmr": "webpack -w --config webpack.serverHMR.js",
		"#dev-all": "Dev mode: watch for server changes and start UI server",
		"dev-all": "rm dist/* && npm run watch-server-hmr & sleep 5 && npm start",
		"build": "webpack",
		"heroku-postbuild": "npm run build && ln -sf ../node_modules/bootstrap/dist public/bootstrap"
	},
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"bootstrap": "^4.6.0",
		"dotenv": "^8.2.0",
		"express": "^4.16.4",
		"http-proxy-middleware": "^1.0.6",
		"isomorphic-fetch": "^3.0.0",
		"nodemon": "^1.18.9",
		"prop-types": "^15.6.2",
		"react": "^16.7.0",
		"react-bootstrap": "^1.4.3",
		"react-dom": "^16.7.0",
		"react-icons": "^4.1.0",
		"react-router-bootstrap": "^0.25.0",
		"react-router-dom": "^4.3.1",
		"react-select": "^2.4.4",
		"serialize-javascript": "^4.0.0",
		"source-map-support": "^0.5.19",
		"url-search-params": "^1.1.0",
		"webpack": "^4.46.0",
		"webpack-merge": "^5.7.3",
		"webpack-dev-middleware": "^3.5.0",
		"webpack-hot-middleware": "^2.24.3",
		"webpack-node-externals": "^1.7.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.2.3",
		"@babel/preset-react": "^7.0.0",
		"babel-loader": "^8.0.5",
		"eslint": "^5.12.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.2",
		"eslint-plugin-react": "^7.12.3",
		"webpack-cli": "^3.2.1"
	}
}